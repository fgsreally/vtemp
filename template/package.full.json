{
  "name": "vue",
  "version": "0.0.0",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://localhost:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://localhost:4173/ 'cypress run --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@fgsreally/vanime": "^0.0.2",
    "@types/nprogress": "^0.2.0",
    "@vformore/component": "^1.0.1",
    "@vformore/core": "^1.0.3",
    "@vformore/model": "^1.0.5",
    "@vitejs/plugin-legacy": "^2.3.0",
    "@vueuse/core": "^9.4.0",
    "axios": "^1.1.3",
    "cors": "^2.8.5",
    "element-plus": "^2.2.19",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.21",
    "rollup-plugin-visualizer": "^5.8.3",
    "sass": "^1.56.0",
    "sfc-server": "^0.0.1",
    "unocss": "^0.46.3",
    "unplugin-auto-import": "^0.11.4",
    "unplugin-vue-components": "^0.22.9",
    "vite-federation": "1.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-inspect": "^0.7.7",
    "vite-plugin-pwa": "^0.13.2",
    "vite-plugin-sfcmore": "^1.0.6",
    "vite-plugin-vue-inspector": "^1.1.3",
    "vitepress": "1.0.0-alpha.27",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.56",
    "@unocss/preset-icons": "^0.46.3",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^10.7.0",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.14.0",
    "terser": "^5.15.1",
    "typescript": "~4.7.4",
    "unplugin-vue-macros": "^0.16.0",
    "vite": "^3.0.9",
    "vite-plugin-dts": "^1.7.0",
